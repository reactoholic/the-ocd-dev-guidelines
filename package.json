{
  "name": "the-ocd-dev-guidelines",
  "version": "0.1.0",
  "private": true,
  "description": "Guidelines for my OCD developers out there",
  "author": "Petar [reactoholic] Kolev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/reactoholic/the-ocd-dev-guidelines"
  },
  "engines": {
    "node": "^20",
    "npm": "^10"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --config .prettierrc",
    "lint": "next lint",
    "prepare": "npx husky",
    "start": "next start",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/joy": "^5.0.0-beta.48",
    "@mui/material": "^6.1.6",
    "@reduxjs/toolkit": "^2.3.0",
    "next": "14.2.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.17",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.0.0"
  }
}
